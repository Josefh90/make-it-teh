{
  "manifest_version": 3,
  "name": "make-it-teh",
  "version": "0.1.0",
  "description": "Ein einfacher Test",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
},
  "permissions": ["storage"],
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "run_at": "document_idle"
  }
]

}